Very few usage on Arrays in go.

package main

import "fmt"

func main() {
	fmt.Println("Welcome to arrays in golang")

	var fruitList [4]string

	fruitList[0] = "Apple"
	fruitList[1] = "Banana"
	fruitList[3] = "Papaya"

	fmt.Println("FruitList is: ", fruitList)

	var vegList = [4]string{"potato", "beans", "mushrooms"}

	fmt.Println("VegyList is: ", vegList)
	fmt.Println("Length of VegyList is: ", len(vegList))

}
